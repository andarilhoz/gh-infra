# GitHub fine-grained PAT with `administration:write` scope on the target repo
GITHUB_PAT=ghp_your_token_here

# Target repository in owner/repo format
GITHUB_REPOSITORY=myorg/myrepo

# Prefix for runner names (hostname is appended automatically)
RUNNER_NAME=flutter-runner

# Flutter version to bake into the image (override to upgrade)
FLUTTER_VERSION=3.41.1
